<section class="createTeamContent">
    <header class="flex">
        <h1 class="yourTeams">TEAMS</h1>

        <section class="flex navUser">
            <div class="username"><a style="color: white"
                    href="/dashboard/<%= user._id %>"><%= user.name.toUpperCase() %></a></div>
            <div class="navUserLogSet"><a style="color: #7e7e7e;" href="/logout">LOGOUT</a></div>
            <div style="color: #7e7e7e; padding-bottom: 1px;">|</div>
            <div class="navUserLogSet"><a style="color: #7e7e7e;" href="/update/<%= user._id %>">SETTINGS</a></div>
        </section>
    </header>

    <a href="/dashboard/<%= user._id %>">
        <svg xmlns="http://www.w3.org/2000/svg" width="6%" height="6%" fill="" class="bi bi-arrow-left back"
            viewBox="0 0 16 16">
            <path fill-rule="evenodd"
                d="M15 8a.5.5 0 0 0-.5-.5H2.707l3.147-3.146a.5.5 0 1 0-.708-.708l-4 4a.5.5 0 0 0 0 .708l4 4a.5.5 0 0 0 .708-.708L2.707 8.5H14.5A.5.5 0 0 0 15 8z" />
        </svg>
    </a>

    <form class="center mt-4" action="/createoneteam" method="POST">

        <h2 class="center"><%= user.name.toUpperCase() %>S NEW TEAM</h2>

        <div class="center mt-3">
            <!-- <label for="name">Name:</label> -->
            TEAMNAME: <input type="text" name="name" id="name"><br>
        </div>
        <input type="hidden" name="userId" value="<%= user._id %>">
        <!-- <input type="submit" value="Add team"> -->
        <div class="center mt-4">
            <button class="btn btn-outline-success updateUserFormButtons" type="submit">CREATE TEAM</button>
        </div>

    </form>


</section>