<%= team.name %> 
<form action="/updatesaveteam/<%= team._id %>" method="post">
    name: <input type="text" name="name" id="name" value="<%= team.name %>">
    <button type="submit">save</button>
</form>

<% teammembers.forEach(member => { %>
    
    <% if (member._id.toString() != team.admin.toString()) { %> 
        <div><%= member.name %> <a href="/kickmember/<%= team._id %>/<%= member._id %>">kick</a></div> 
    <% } %> 
<% }) %> 

<a href="/deleteteam/<%= team._id %>">delete team</a>

<a href="/teamdashboard/<%= team._id %>"><div>BACK</div></a>